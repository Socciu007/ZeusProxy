    <div class="packages">
        <div class="packages-row-head">
            <div class="head-pricing"></div>
            <% packages.forEach(function(item) { %>
                <div class="pricingItem <%= item["isPopular"] ? "pricingItemMost" : "" %>">
                    <div class="mostPopular">
                        <%= item["isPopular"] ? "Most popular" : "" %>
                    </div>
                </div>
                <% }) %>
        </div>
        <div class="packages-row-body">
            <img src="/images/big-sale.png" class="big-sale" />
