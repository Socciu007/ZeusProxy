                <div class="packageName invisible">
                    plan
                </div>
                <a href="https://app.zeusproxy.com/" target="_blank" class="btnOrderBlue invisible">
                    Start Now
                </a>
                <div class="packagePrice">
                    Price
                </div>
                <div class="package">
                    GB
                </div>
                <div class="package">
                    Port quantity
                </div>
                <div class="package">
                    Duration
                </div>
            </div>
            <% packages.slice(0, 5).forEach(function(item) { %>
                <div class="pricingItem <%= item["isPopular"] ? "pricingItemMost" : "" %>">
                    <div class="packageName">
                        <%= item.title%>
                    </div>
                    <a href="https://app.zeusproxy.com/" target="_blank" class="btnOrderBlue">
                        Start Now
                    </a>
                    <div class="packagePrice">
                        <div class="price2">
                            <%= item["currency"]%><%= item["price"]%>
                        </div>
                        <div class="price">
                            <%= item["currency"]%><%= (item["price"] * 0.6).toFixed(1) %>
                        </div>
                        <div class="unit">/ GB</div>
                    </div>
                    <div class="package">
                        <%=item["bandwidth"]["from"]%> - <%= item["bandwidth"]["to"]%> GB
                    </div>
                    <div class="package">
                        <%=item["port-quantity"]%>
                    </div>
                    <div class="package">
                        <%=item["duration"]%> days
